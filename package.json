{
  "name": "get-your-job",
  "version": "1.0",
  "description": "Plataforma de Vagas de Emprego",
  "main": "Index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:coverage": "yarn test --coverage",
    "test:debugger": "node --inspect node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test --watch",
    "test:snapshot": "jest --updateSnapshot",
    "lint": "tslint -p .",
    "lint:fix": "yarn lint --fix",
    "coverage": "yarn test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DenisdS/get-your-job"
  },
  "keywords": [],
  "author": "Denis Santos",
  "homepage": "https://github.com/DenisdS/get-your-job/blob/main/README.md",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "~7.2",
    "chalk": "~2.4",
    "core-js": "^3.6.5",
    "css-loader": "~0.28",
    "dotenv": "~4.0",
    "file-loader": "~1.1",
    "fs-extra": "~5.0",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "~2.2",
    "lodash": "~4.17",
    "moment": "~2.24",
    "moment-timezone": "~0.5",
    "node-sass": "~4.13",
    "object-assign": "~4.1",
    "postcss-flexbugs-fixes": "~3.3",
    "postcss-loader": "~2.1",
    "promise": "~8.0",
    "react": "~16.12",
    "react-circular-progressbar": "~2.0.3",
    "react-dev-utils": "~5.0",
    "react-dom": "~16.12",
    "react-ga": "~2.7",
    "react-router": "~4.3",
    "react-router-dom": "~4.3",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "~6.0",
    "stats.js": "^0.17.0",
    "unfetch": "~4.1",
    "url-loader": "~0.6"
  },
  "devDependencies": {
    "@types/enzyme": "~3.1",
    "@types/enzyme-adapter-react-16": "~1.0",
    "@types/jest": "~23.3",
    "@types/node": "~9.6",
    "@types/react": "~16.9",
    "@types/react-circular-progressbar": "~1.1",
    "@types/react-dom": "~16.9",
    "@types/react-lazyload": "~2.6",
    "@types/react-router-dom": "~4.3",
    "@types/react-test-renderer": "~16.9",
    "@types/scriptjs": "~0.0",
    "args-parser": "~1.1",
    "babel-cli": "~6.26",
    "babel-core": "~6.26",
    "babel-jest": "~23.6",
    "babel-plugin-syntax-dynamic-import": "~6.18",
    "babel-preset-env": "~1.7",
    "babel-preset-es2015": "~6.24",
    "babel-preset-react": "~6.24",
    "case-sensitive-paths-webpack-plugin": "~2.1",
    "chance": "~1.0",
    "enzyme": "~3.10",
    "enzyme-adapter-react-16": "~1.15",
    "extract-text-webpack-plugin": "~3.0",
    "html-webpack-externals-plugin": "~3.8",
    "html-webpack-plugin": "~2.30",
    "husky": "~1.3",
    "jest": "~23.6",
    "jest-localstorage-mock": "~2.2",
    "jest-preset-typescript": "~1.2",
    "npm-run-all": "^4.1.5",
    "react-docgen-typescript-loader": "~3.6",
    "react-docgen-typescript-webpack-plugin": "~1.1",
    "react-render-debugger": "~1.0",
    "react-test-renderer": "~16.12",
    "scriptjs": "~2.5",
    "source-map-loader": "~0.2",
    "style-loader": "~0.23",
    "sw-precache-webpack-plugin": "~0.11",
    "ts-jest": "~22.4",
    "ts-loader": "~3.5",
    "tslint": "~5.20",
    "tslint-loader": "~3.5",
    "tslint-react": "~3.6.0",
    "tslint-sonarts": "~1.9.0",
    "typescript": "~3.7",
    "uglify-js": "~3.7",
    "uglifyjs-webpack-plugin": "~1.3",
    "webpack": "~3.12",
    "webpack-dev-server": "~2.11",
    "webpack-manifest-plugin": "~1.3"
  },
  "jest": {
    "automock": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/Index.tsx",
      "!src/**/*.d.ts",
      "!src/**/__tests__/mock/*",
      "!src/**/__mock__/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 50,
        "functions": 40,
        "lines": 60
      }
    },
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "window": {},
      "document": {},
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
