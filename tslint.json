{
  "extends": [
    "tslint-react",
    "tslint-sonarts"
  ],
  "rules": {
    "no-floating-promises": false,
    "align": [
      false
    ],
    "no-use-of-empty-return-value": false,
    "ban": false,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "max-line-length": [
      true,
      150
    ],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "ordered-imports": [
      true,
      {
        "module-source-path": "basename",
        "grouped-imports": true,
        "groups": [{
            "name": "react",
            "match": "^react.*",
            "order": 1
          },{
            "name": "Internal Style",
            "match": "^[.].*(css|scss)",
            "order": 6
          }, {
            "name": "External Style",
            "match": "^.*(css|scss)",
            "order": 5
          }, {
            "name": "Internal Modules",
            "match": "^@app.*",
            "order": 3
          }, {
            "name": "relative dir",
            "match": "^[.].*",
            "order": 4
          }, {
            "name": "node_modules",
            "match": ".*",
            "order": 2
          }
        ]
      }
    ],
    "no-any": true,
    "no-arg": true,
    "no-invalid-await": false,
    "no-identical-functions": false,
    "no-bitwise": true,
    "no-console": [
      true,
      "log",
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": true,
    "object-literal-sort-keys": false,
    "no-construct": true,
    "no-debugger": false,
    "no-duplicate-string": false,
    "no-useless-cast": false,
    "consecutive-overloads": false,
    "no-ignored-return": false,
    "cognitive-complexity": false,
    "no-redundant-boolean": false,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "member-access": false,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "never"
    ],
    "switch-default": false,
    "trailing-comma": [
      false
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
